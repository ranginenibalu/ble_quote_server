void inertial_sensing_init(void)
{
	uint8_t dev_id =0;    
  uint8_t config_regs_accel[9] = { 0x9F, 0x00, 0x4F, 0x00, 0x00, 0x00, 0x10, 0x00, 0x00}, config_regs_gyro[6] = {0xA0, 0x4F, 0X20, 0X00, 0X90, 0X00} ;
  if(_lsm9ds0_init())  
	{
		dev_id = _lsm9ds0_read_id();
		if(dev_id == 0x49 )    
		{
			
			
			 _lsm9ds0_activate_accelerometer(config_regs_accel);
			  nrf_delay_us(400);
			
			 _lsm9ds0_activate_gyroscope(config_regs_gyro);			 
		}
	}
	
}








 
